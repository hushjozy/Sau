{
  "expo": {
    "name": "Sau",
    "slug": "Sau",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/sau-logo.png",
    "scheme": "sau",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/sau-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lafarge.sau",
      "appleTeamId": "U4KGHX3JP9"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/sau-logo.png",
        "backgroundColor": "#000000"
      },
      "package": "com.lafarge.sau"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "tsconfigPaths": true,
      "typedRoutes": true
    }
  }
}
